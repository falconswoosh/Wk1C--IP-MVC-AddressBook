@{
  Layout = "_Layout";
}

<div class="container">

<h1 id="allContacts">Contacts List</h1>
<ul>
  @foreach (var contact in Model)
  {
    <li><a href='/contacts/@contact.GetId()'>@contact.GetLastName(), @contact.GetFirstName()</a></li>
  }
</ul>

</div>
  <a href="/contacts/new">Add Another Contact</a>
  <a href="/contacts">View all Contacts</a>
  <a href="/contacts/clear">Clear All Contacts</a>
